{
    "name": "libdaisy",
    "version": "0.0.1",
    "build": {
        "extraScript": "./platformio_extra_build_script.py",
        "srcDir" : "./",
        "srcFilter": [
            "+<core>",
            "-<core/startup_stm32h750xx.s>",
            "+<src/>",
            "+<src/sys>",
            "+<src/sys/dma>",
            "+<src/sys/fatfs>",
            "+<src/sys/system>",
            "+<src/dev/>",
            "+<src/dev/codec_ak4556>",
            "-<src/dev/codec_wm8731.h>",
            "-<src/dev/codec_wm8731.c>",
            "+<src/hid/audio>",
            "+<src/dev/sdram>",
            "+<src/per/dac>",
            "+<src/per/gpio>",
            "+<src/per/qspi>",
            "+<src/per/sai>",
            "+<src/per/sdmmc>",
            "+<src/per/tim>",
            "+<src/dev/sr_4021>",
            "+<src/util>",
            "+<src/usbd>",
            "+<src/sys/system_stm32h7xx>",
            "+<src/sys/stm32h7xx_hal_conf.h>"
        ],
        "flags": [
            "-std=gnu++14",
            "-fno-exceptions",
            "-mfloat-abi=hard",
            "-mfpu=fpv4-sp-d16",
            "-DARM_MATH_CM7",
            "-DCORE_CM7",
            "-DSTM32H750xx",
            "-DSTM32H750IB",
            "-DARM_MATH_CM7",
            "-Dflash_layout",
            "-DHSE_VALUE=16000000",
            "-DUSE_HAL_DRIVER",
            "-Isrc/sys/stm32h7xx_hal_conf.h",
            "-Isrc",
            "-Isrc/sys",
            "-Isrc/sys/dma",
            "-Isrc/sys/fatfs",
            "-Isrc/sys/system",
            "-Isrc/dev/codec_ak4556",
            "-Isrc/hid/audio",
            "-Isrc/dev/sdram",
            "-Isrc/per/dac",
            "-Isrc/per/gpio",
            "-Isrc/per/qspi",
            "-Isrc/per/sai",
            "-Isrc/per/sdmmc",
            "-Isrc/per/tim",
            "-Isrc/dev/sr_4021",
            "-Isrc/util",
            "-Isrc/usbd",
            "-Isrc/usbh",
            "-Isrc/sys/system_stm32h7xx",
            "-I $PROJECT_DIR/packages/framework-stm32cubeh7/Middlewares/Third_Party/FatFs/src"
        ]
    }
  }